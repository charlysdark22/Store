{
  "name": "tech-store-cuba",
  "version": "1.0.0",
  "description": "E-commerce platform for computers, laptops, phones and accessories in Cuba",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "seed": "node scripts/seed-data.js",
    "setup": "chmod +x setup.sh && ./setup.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5",
    "nodemailer": "^6.9.4",
    "stripe": "^13.5.0",
    "qrcode": "^1.5.3",
    "i18next": "^23.4.4",
    "react-i18next": "^13.2.0",
    "handlebars": "^4.7.8",
    "web-push": "^3.6.6",
    "speakeasy": "^2.0.0",
    "redis": "^4.6.10",
    "bull": "^4.11.4",
    "socket.io": "^4.7.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3"
  },
  "keywords": ["ecommerce", "cuba", "technology", "store", "react", "nodejs"],
  "author": "Tech Store Cuba",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}